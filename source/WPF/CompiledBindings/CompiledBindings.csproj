<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netcoreapp3.0;net35;net40</TargetFrameworks>
		<Product>CompiledBindings</Product>
		<Company>levitali</Company>
		<Version>1.0.16</Version>
		<UseWPF>true</UseWPF>
		<LangVersion>latest</LangVersion>
