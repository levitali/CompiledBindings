		<WarningsAsErrors>nullable</WarningsAsErrors>
		<OutputPath>..\..\..\bin\wpf\lib</OutputPath>
		<NoWarn>1701;1702;netsdk1138</NoWarn>
	</PropertyGroup>

</Project>